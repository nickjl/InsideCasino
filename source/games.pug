extends includes/layout

append css
    style
        include ../app/css/games/games.css


block content

    include includes/svg/svg
    include includes/casino-card/casino-card


    .wrapper


        section.game-info
            .d-flex.flex-column.flex-sm-row
                .game-info__img.d-flex.justify-content-center.px-2.my-sm-4
                    img(src="assets/images/games-assets/game00.png")

                .game-info__title.d-flex.flex-column.px-4.py-sm-4.justify-content-around.align-items-center
                    h1 STARBURST
                    p offered by
                        a.pl-1(href="") CRYPTOLOGIC
                    h2 5 5 5 5 5

                .game-info__play.ml-sm-auto.d-flex.align-items-center.pr-sm-4.justify-content-center
                    a(href="") Play now

        section.game-summary
            .d-flex.flex-column.flex-lg-row.justify-content-around
                .d-flex.justify-content-center.align-items-center.game-summary__width.px-md-4
                    img.img-fluid.game-summary__demo(src="assets/images/games/starburst_play.jpg")

                .game-summary__attributes.d-flex.flex-column.justify-content-center.game-summary__width.py-4
                    .flex-row.d-flex.justify-content-around.px-2.flex-wrap
                        .game-summary__icons--box
                            svg.tablet(viewBox="0 0 110 110")
                                use(xlink:href="#i_tablet" x="0" y="0")
                            p.p-1.p-md-2 Mobile Version
                        .game-summary__icons--box
                            svg.vip(viewBox="0 0 110 110")
                                use(xlink:href="#i_vip" x="0" y="0")
                            p.p-1.p-md-2 5 Reels
                        .game-summary__icons--box.icon-disabled
                            svg.live(viewBox="0 0 110 110")
                                use(xlink:href="#i_live" x="0" y="0")
                            p.p-1.p-md-2 Free Spins
                        .game-summary__icons--box
                            svg.mobile(viewBox="0 0 110 110")
                                use(xlink:href="#i_mobile" x="0" y="0")
                            p.p-1.p-md-2 Video Games
                        .game-summary__icons--box
                            svg.desk(viewBox="0 0 110 110")
                                use(xlink:href="#i_desk" x="0" y="0")
                            p.p-1.p-md-2 Progressive Slot
                        .game-summary__icons--box
                            svg.paypal(viewBox="0 0 110 110")
                                use(xlink:href="#i_paypal" x="0" y="0")
                            p.p-1.p-md-2 BONUS Games
                    .game-summary__attributes--list
                        h2 Attributes
                        ul.d-flex.flex-row.flex-wrap.px-2
